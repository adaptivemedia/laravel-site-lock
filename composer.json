{
    "name": "adaptivemedia/laravel-site-lock",
    "description": "Lock access to a site",
    "keywords": [
        "adaptivemedia",
        "laravel-site-lock",
        "demo mode"
    ],
    "homepage": "https://github.com/adaptivemedia/laravel-site-lock",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Eliasson",
            "email": "victor@adaptivemedia.se",
            "homepage": "https://adaptivemedia.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": ">9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": ">6.0",
        "phpunit/phpunit": ">9.0"
    },
    "autoload": {
        "psr-4": {
            "Adaptivemedia\\SiteLock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adaptivemedia\\SiteLock\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adaptivemedia\\SiteLock\\SiteLockServiceProvider"
            ]
        }
    }
}
